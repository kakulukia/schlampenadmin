-load static compress raven

!!!5
html(lang='de')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='initial-scale=1.0')
    //link(rel="shortcut icon", href="{% static 'img/favicon.ico' %}")
    title
      block title
        | Give me a title
    meta(name="viewport", content="initial-scale=1.0")
    link(href="https://fonts.googleapis.com/css?family=Barrio", rel="stylesheet")

    -compress css
      link(rel="stylesheet", href="{% static 'css/bootstrap.min.css' %}")
      link(rel="stylesheet", href="{% static 'css/font-awesome.css' %}")
      link(rel='stylesheet', type='text/x-sass', href='{% static "sass/styles.sass" %}')

body
  #app
    header
      block header
    main
      block content
    footer
      block footer


  script(src="https://cdn.ravenjs.com/3.15.0/vue/raven.min.js", crossorigin="anonymous")
  script Raven.config('{% sentry_public_dsn %}').install();
  -compress js
    script(src="{% static 'js/jquery-3.1.1.js' %}")
    script(src="{% static 'js/tether.min.js' %}")
    script(src="{% static 'js/bootstrap.min.js' %}")
    script(src="{% static 'js/vue.js' %}")
    block extra_js
